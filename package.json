{
  "name": "@ricardo17coelho/vue-simple-acl",
  "type": "module",
  "version": "1.0.1",
  "description": "A simple unopinionated Vue plugin for managing user roles and permissions, access-control list (ACL) and role-based access control (RBAC).",
  "author": "Victory Osayi <lavictorybiz@gmail.com>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Ricardo Coelho",
      "email": "devel@rmorgado.ch",
      "url": "https://github.com/ricardo17coelho"
    }
  ],
  "homepage": "https://github.com/ricardo17coelho/vue-simple-acl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardo17coelho/vue-simple-acl.git"
  },
  "bugs": {
    "url": "https://github.com/ricardo17coelho/vue-simple-acl/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "vue 2",
    "vue3",
    "vue 3",
    "acl",
    "rbac",
    "vue-alc",
    "vue alc",
    "vue-simple-alc",
    "vue simple alc",
    "vue simple alc next",
    "Access-Control List",
    "Role Based Access Control",
    "javacript",
    "typescript"
  ],
  "main": "./dist/vue-simple-acl-next.umd.cjs",
  "module": "./dist/vue-simple-acl-next.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "types/*",
    "web-types.json"
  ],
  "scripts": {
